{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        // path relative to blend4web (cwd)
        "doc_src/api_doc/jsdoc_resources/plugins/plugins_local",
        "doc_src/api_doc/jsdoc_resources/plugins/deprecated_ext" 
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
