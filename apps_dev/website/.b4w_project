[info]
author = Blend4Web
name = website
title = Website
icon = .b4w_icon.png

[paths]
assets_dirs = deploy/assets/website;
blend_dirs = blender/website;
blender_exec = blender
build_dir = deploy/apps/website
deploy_dir = ../website_b4w/website/blend4web/static/blend4web/3d/website/

[compile]
apps = 
css_ignore = 
engine_type = copy
ignore = 
js_ignore = 
optimization = advanced
use_physics = False
use_smaa_textures = False
version = 

[deploy]
assets_path_dest = assets
assets_path_prefix = /static/blend4web/3d/website/assets
override = True
ignore = contest.html;logo_3d.html
